    "repository": "github:web-illinois/ilw-call-to-action",
    "private": false,
    "license": "MIT",
    "version": "1.0.0-beta",
    "type": "module",
    "files": [
        "src/**",
        "dist/**",
        "builder/**"
    ],
    "exports": {
        ".": {
            "import": "./src/ilw-call-to-action.js",
            "require": "./dist/ilw-call-to-action.cjs",
            "default": "./dist/ilw-call-to-action.js"
        }
    },
    "main":"./dist/ilw-call-to-action.js",
    "scripts": {
        "dev": "vite",
        "build": "vite build --config vite.build.config.js --emptyOutDir",
        "preview": "vite preview",
        "prepack": "npm run build"
    },
    "dependencies": {
        "lit": "3.1.3"
    },
    "devDependencies": {
        "vite": "^5.2.0",
        "postcss-nested": "^6.2.0"
    }
}
