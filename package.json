{
    "name": "@illinois-toolkit/ilw-call-to-action",
    "description": "Illinois Toolkit: The call to action component is used to prompt visitors to perform an action or a few related actions, such as subscribing to a newsletter or beginning an application.",
    "repository": "github:web-illinois/ilw-call-to-action",
    "private": false,
    "license": "MIT",
    "version": "1.1.0",
    "type": "module",
    "files": [
        "src/**",
        "dist/**",
        "builder/**"
    ],
    "exports": {
        ".": {
            "import": "./dist/ilw-call-to-action.js",
            "require": "./dist/ilw-call-to-action.cjs",
            "default": "./dist/ilw-call-to-action.js"
        },
        "./ilw-call-to-action.css": {
            "import": "./dist/ilw-call-to-action.css"
        }
    },
    "main": "./dist/ilw-call-to-action.js",
    "module": "./dist/ilw-call-to-action.js",
    "scripts": {
        "dev": "vite",
        "build": "tsc --noEmit && vite build --config vite.transpile.config.ts --emptyOutDir && vite build --config vite.build.config.ts --emptyOutDir",
        "preview": "vite preview",
        "prepack": "npm run build",
        "test:axe": "playwright test",
        "test:axe:github": "playwright test --config playwright.ci.config.ts",
        "test": "vitest run --browser.headless",
        "test:browser": "vitest watch --browser chromium"
    },
    "dependencies": {
        "lit": "3.1.3"
    },
    "devDependencies": {
        "@illinois-toolkit/ilw-core": "^1.0.5",
        "@types/node": "^22.18.6",
        "prettier": "^3.6.2",
        "typescript": "^5.9.2",
        "vite": "7.1.7",
        "vite-plugin-dts": "^4.5.4",
        "@playwright/test": "^1.54.1",
        "@vitest/browser": "^3.2.4",
        "playwright": "^1.54.1",
        "postcss-nested": "^6.2.0",
        "vitest-browser-lit": "^0.1.0"
    }
}
